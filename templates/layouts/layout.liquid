    <!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <!-- Title block allowing child pages to override -->
        <title>{% block title %}Foundation 5.3{% endblock %}</title>
        <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
        <!-- Styles -->
        {{ "foundation.css" | asset_url | stylesheet_tag }}
        {{ "app.css" | asset_url | stylesheet_tag }}
        {{ "modernizr.js" | asset_url | script_tag }}
       
        <!-- Style block allowing child pages to inject their own styles -->
       <!--  {% block styles %}{% endblock %} -->
    </head>
    <body>
        <div id="wrapper">
            <!-- Include -->
            {% include "header" %}
            <!-- Content block, child page inject content -->
            {% block content %}{% endblock %}
        </div>
        <!-- Script block allowing child pages to inject their own scripts -->
      <!--   {% block scripts %}{% endblock %} -->
        {{ "jquery.js" | asset_url | script_tag }}
        {{ "foundation.min.js" | asset_url | script_tag }}
        {{ "app.js" | asset_url | script_tag }}
    </body>
</html>